public String reverse(String input) {

	    if (input == null) {
	        return input;
	    }

	    String output = "";

	    for (int i = input.length() - 1; i >= 0; i--) {
	        output = output + input.charAt(i);
	    }

	    return output;
	}
}
  @Test
	void testStringreversee() {
		Stringreverse example = new Stringreverse();
                        assertEquals("tac", example.reverse("cat"));
}
